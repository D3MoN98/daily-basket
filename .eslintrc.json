{
    "env": {
        "browser": true,
        "es2020": true
    },
    "extends": [
        "plugin:vue/essential",
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 11,
        "sourceType": "module"
    },
    "plugins": [
        "vue"
    ],
    "rules": {
        "no-undef": 0,
        "import/no-extraneous-dependencies": 0,
        "import/no-unresolved": 0,
        "import/extensions": 0,
        "space-before-function-paren": 0,
        "indent": 0,
        "max-len": 0,
        "camelcase": 0,
        "no-console": 0,
    }
}
